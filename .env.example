# Server Configuration
PORT=3000
NODE_ENV=development
CORS_ORIGIN=http://localhost:3001

# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key
AWS_REGION=us-east-1
DYNAMODB_TABLE_PREFIX=swapjeet_
S3_BUCKET_NAME=swapjeet-assets

# Authentication
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
JWT_SECRET=your_jwt_secret
JWT_EXPIRY=24h

# External APIs
ZAPPER_API_KEY=your_zapper_api_key
ETHERSCAN_API_KEY=your_etherscan_api_key
COINGECKO_API_KEY=your_coingecko_api_key

# Cache Configuration
CACHE_TTL=900 # 15 minutes in seconds
LEADERBOARD_CACHE_TTL=3600 # 1 hour in seconds

# Rate Limiting
RATE_LIMIT_WINDOW=900000 # 15 minutes in milliseconds
RATE_LIMIT_MAX_REQUESTS=100

# WebSocket Configuration
WS_PING_INTERVAL=300000 # 5 minutes in milliseconds
WS_PING_TIMEOUT=10000 # 10 seconds in milliseconds

{
  "name": "swapjeet-be",
  "version": "1.0.0",
  "description": "SwapJeet Backend - Track Ethereum wallet fumbles and manage leaderboards",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && cp src/swagger.yaml dist/",
    "setup-db": "ts-node scripts/setup-db.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "ethereum",
    "wallet-tracking",
    "defi",
    "analytics"

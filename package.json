    "@types/js-yaml": "^4.0.9",
    "@types/swagger-ui-express": "^4.1.8",
    "aws-sdk": "^2.x",
    "axios": "^1.8.3",
    "bcryptjs": "^2.4.3",
    "canvas": "^3.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "^6.13.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "google-auth-library": "^8.7.0",
    "helmet": "^6.0.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "socket.io": "^4.6.1",
